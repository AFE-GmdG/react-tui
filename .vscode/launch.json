{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Demo",
      "program": "${workspaceFolder}/dist/index.js",
      "runtimeArgs": [
        "--preserve-symlinks",
      ],
      "cwd": "${workspaceFolder}",
      "args": [],
      "preLaunchTask": "build-demo",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceFolder}/src/*",
        "webpack:///*": "*",
      },
      "console": "integratedTerminal",
      "presentation": {
        "hidden": false,
        "group": "01 - Demo",
        "order": 1,
      },
    },
  ]
}
