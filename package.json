{
  "name": "@afegmdg/react-tui",
  "version": "0.0.2",
  "description": "A React custom renderer for the terminal.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist *.tgz *.log",
    "build": "webpack --mode=production",
    "prebuilddev": "rimraf ./dist *.tgz *.log",
    "builddev": "webpack --mode=development",
    "prebuilddemo": "rimraf ./dist *.tgz *.log",
    "builddemo": "webpack --mode=development --env=demo",
    "demo": "node .",
    "prepack": "rimraf ./dist *.tgz *.log && webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "clean": "rimraf ./dist *.tgz *.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AFE-GmdG/react-tui.git"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "keywords": [
    "react",
    "tui",
    "curses"
  ],
  "author": "AFE-GmdG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AFE-GmdG/react-tui/issues"
  },
  "homepage": "https://github.com/AFE-GmdG/react-tui#readme",
  "dependencies": {
    "ansi-escapes": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.8",
    "@types/react": "18.0.17",
    "@types/react-reconciler": "0.28.0",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "react": "18.2.0",
    "react-reconciler": "0.29.0",
    "rimraf": "3.0.2",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  }
}
